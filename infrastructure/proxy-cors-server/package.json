    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.0",
    "@types/node": "^16.11.6",
    "@types/supertest": "^2.0.12",
    "body-parser": "^1.20.2",
    "jest": "^29.5.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.3.3",
    "typescript": "^4.4.4"
  },
  "keywords": [
    "express",
    "proxy",
    "cors"
  ],
