{
  "name": "express-proxy-server",
  "version": "1.0.0",
  "description": "An Express proxy server for IGDB API",
  "main": "server.ts",
  "scripts": {
    "build": "tsc -p .",
    "start": "node ./build/proxy-cors-server/server.js",
    "dev": "nodemon -e ts --watch ./ --exec \"npm run build\"",
